using System;  
  public class SwapExample  
   {  
     public static void Main(string[] args)  
      {  
       int  a=1, b=2;            
       Console.WriteLine("Before swap a= "+a+" b= "+b);    
       a=a+b; //a=3 (1+2)      
       b=a-b; //b=1 (3-2)      
       a=a-b; //a=2 (3-1)   
       Console.Write("After swap a= "+a+" b= "+b);       
      }  
  }  
